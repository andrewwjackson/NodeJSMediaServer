{
    "server": {
        "port": 8087,
        "ip": "127.0.0.1",
        "customheaders": [{
                "arrnode": "NodeJSMediaServer"
            },
            {
                "X-Powered-By": "Hopes and Dreams"
            }
        ],
        "sqlEnabled": true,
        "fsEnabled": true,
        "logToConsole": false,
        "defaultPixelCookie": "pixel_cookie",
        "pixelCookieDomain": "mydomain.com"
    },
    "sql": {
        "default": "web",
        "conn": {
            "master": {
                "server": "127.0.0.1",
                "database": "Sitecore.Master",
                "user": "dbuser",
                "password": "dbpass",
                "options": {
                    "trustedConnection": true,
                    "encrypt": true,
                    "enableArithAbort": true,
                    "trustServerCertificate": true
                }
            },
            "web": {
                "server": "127.0.0.1",
                "database": "Sitecore.Web",
                "user": "dbuser",
                "password": "dbpass",
                "options": {
                    "trustedConnection": true,
                    "encrypt": true,
                    "enableArithAbort": true,
                    "trustServerCertificate": true
                }
            },
            "core": {
                "server": "127.0.0.1",
                "database": "Sitecore.Core",
                "user": "dbuser",
                "password": "dbpass",
                "options": {
                    "trustedConnection": true,
                    "encrypt": true,
                    "enableArithAbort": true,
                    "trustServerCertificate": true
                }
            },
            "track": {
                "server": "127.0.0.1",
                "database": "PixelTrackDB",
                "user": "dbuser",
                "password": "dbpass",
                "options": {
                    "trustedConnection": true,
                    "encrypt": true,
                    "enableArithAbort": true,
                    "trustServerCertificate": true
                }
            }
        }
    },
    "filesys": {
        "rootpath": "c:\\inetpub\\wwwroot\\mysite\\website\\fs"
    },
    "caching": {
        "maxage": 31536000,
        "cachability": "public",
        "vary": "Accept-Encoding"
    }
}